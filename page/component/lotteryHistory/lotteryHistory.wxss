/* page/component/lotteryHistory/lotteryHistory.wxss */
.history-list{
  border: 1px solid #ddd;
  border-radius: 6px;
  margin: 10rpx 5%;
  background: #fff;
  padding: 30rpx 0;
  box-shadow: 0px 0px 5px #ddd;
}
.history-list view{
  line-height: 60rpx;
}
.history-list text:first-child{
  display: inline-block;
  width: 32%;
  text-align: right;
  color: #aaa;
  font-size: 14px;
}
.history-list text:last-child{
  display: inline-block;
  width: 68%;
  color: #323232;
  font-size: 14px;
}