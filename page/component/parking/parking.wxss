@import '../../common/common.wxss';
.drop-down {
  height: 85rpx;
  background: #FFf;
}
.drop-down image{
  width: 45rpx;
  height: 45rpx;
  margin: 10px 10rpx 0 10rpx;
  vertical-align: middle;
}
.drop-down text{
  margin-top: 16rpx;
  display: inline-block;
  vertical-align: middle;
  font-size: 30rpx;
}
.list-msg2{
  display: inline-block;
}
.select_box {
  background-color: #fff;
  padding: 0 10rpx;
  /* width: 28%; */
  position: absolute;
  top: 85rpx;
  z-index: 1000;
  overflow: hidden;
  animation: myfirst 0.5s;
  left: 58rpx;  
  border-radius: 10rpx;
}
@keyframes myfirst {
  from {
      height: 0rpx;
  }
  to {
      height: 200rpx;
  }
}
.select_one {
  height: 60rpx;
  line-height: 60rpx;
  font-size: 30rpx;
}


.park-box .box-title{
  display: block;
  text-align: center;
  margin-top: 25rpx;
}
.park-box input{
  display: inline-block;
  width: 9%;
  border: 1px solid #D3D3D3;
  height: 37.25px;
  margin: 0 10rpx;
  text-align: center;
}
.park-box .spot{
  background: #D3D3D3;
  width: 8rpx;
  height: 8rpx;
  display: inline-block;
  margin: 0 10rpx;
  border-radius: 50%;  
}
.park-box .input-box{
  display: flex;
  align-items: center;
  margin-top: 25rpx;
  justify-content: center;
}
.park-box .input-box view{
  display: inline-block;
  width: 9%;
  border: 1px dashed #D3D3D3;
  height: 37.25px;
  margin: 0 10rpx;
  text-align: center;
  padding-top: 6rpx;
}
.input-box view text{
  font-size: 22rpx;
  display: block;
}
.park-box button{
  margin: 45rpx 20rpx;
  background: #DDDDDD;
  color: #fff;
  border-color: #D9D9D9;
}
.search-history{
  padding: 0 10rpx;
}
.search-history text{
  border: 1px solid #D3D3D3;
  padding: 10rpx 10rpx;  
  margin: 0 15rpx;
}



form{
  display: flex;
  justify-content: space-around;
  width:100%;
}
form input{
  height: 94rpx;
  width:94rpx;
  border:1px solid #cdcdcd;
  border-radius: 8rpx;
  display: inline-block;
  line-height: 94px;
  text-align: center;
  font-size: 48rpx;
  color:#323232;
  margin:0 8rpx;
}
form .hidden_ipt{
  height: 0rpx;
  width:0rpx;
  border:none;
  margin:0;
}
.isFocus{
  border-color: #f00 !important;
}